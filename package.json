{
  "name": "kmetija-marosa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:staging": "vite build --mode staging",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:scripts": "tsc --project tsconfig.script.json && node dist-script/migrate-products.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "supabase": "./node_modules/supabase/bin/supabase",
    "deploy": "./deploy-to-netlify.sh",
    "netlify-link": "node netlify-deploy-link.js",
    "deploy:netlify": "npm run build && npm run netlify-link",
    "deploy:direct": "./deploy-netlify.sh",
    "deploy:secure": "./deploy-secure.sh",
    "deploy:edge-functions": "node scripts/deploy-edge-functions.mjs",
    "verify-env": "node scripts/verify-env.mjs",
    "upload:images": "node scripts/upload-images-to-supabase.mjs",
    "predeploy": "npm run verify-env",
    "prebuild": "npm run verify-env",
    "security-check": "npm run verify-env",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "pretest": "npm run verify-env",
    "test:e2e": "playwright test --config=tests/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=tests/config/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=tests/config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=tests/config/playwright.config.ts --debug",
    "test:e2e:chrome": "playwright test --config=tests/config/playwright.config.ts --project=chromium",
    "test:e2e:mobile": "playwright test --config=tests/config/playwright.config.ts --project=mobile-chrome",
    "test:e2e:smoke": "playwright test --config=tests/config/playwright.config.ts --grep @smoke",
    "test:e2e:admin": "playwright test --config=tests/config/playwright.config.ts tests/tests/admin/",
    "test:e2e:checkout": "playwright test --config=tests/config/playwright.config.ts tests/tests/customer/checkout/",
    "test:e2e:payments": "playwright test --config=tests/config/playwright.config.ts tests/tests/customer/payments/",
    "test:e2e:report": "playwright show-report tests/reports/html"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.9",
    "@stripe/react-stripe-js": "^3.6.0",
    "@stripe/stripe-js": "^7.1.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.43.4",
    "@types/uuid": "^10.0.0",
    "framer-motion": "^12.10.1",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "lucide-react": "^0.390.0",
    "puppeteer": "^24.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.23.1",
    "react-router-hash-link": "^2.4.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.17.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-hash-link": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "form-data": "^4.0.2",
    "jsdom": "^26.1.0",
    "netlify-cli": "^20.0.2",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.38",
    "supabase": "^2.20.12",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.4.16",
    "vitest": "^3.1.2"
  }
}
